{
  "name": "rate-a-boulder",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/GronkerLonker/rate-a-boulder.git"
  },
  "scripts": {
    "clean": "rm -rf app",
    "//": "UI BUILD",
    "lint:ui": "eslint public/js",
    "prebuild:ui": "npm run lint:ui",
    "jspm": "jspm bundle src/entry bundle.js",
    "build:ui": "cp -R ./public/. app",
    "//": "SERVER BUILD",
    "lint:server": "eslint src",
    "prebuild:server": "npm run lint:server",
    "build:server": "./node_modules/.bin/babel src -d app",
    "//": "BUILD",
    "prebuild": "npm run clean",
    "build": "npm run build:server && npm run build:ui",
    "//": "TESTING",
    "pretest": "npm run build",
    "test": "echo success",
    "//": "SERVER START",
    "start": "node app/server.js",
    "//": "BUILD + START SERVER WITHOUT UI",
    "restart:server": "npm run build:server && npm run start",
    "//": "BUILD + START",
    "precomplete": "npm run build",
    "complete": "npm run start",
    "//": "WATCHERS",
    "watch:ui": "watch 'npm run build:ui' ./public",
    "//": "NODE ENV HELPER",
    "env": "env"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "lokijs": "^1.3.19",
    "method-override": "^2.3.6",
    "promise": "^7.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^2.13.1",
    "eslint-config-google": "^0.6.0",
    "jspm": "^0.16.39",
    "watch": "^0.19.1"
  },
  "jspm": {
    "dependencies": {},
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
