{
  "name": "rate-a-boulder",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/GronkerLonker/rate-a-boulder.git"
  },
  "scripts": {
    "clean:ui": "",
    "clean:server": "",
    "clean": "rm -rf app",
    "//": "NODE ENV HELPER",
    "lint:ui": "eslint public/js/src",
    "prebuild:ui": "npm run lint:ui",
    "jspm": "jspm install && jspm bundle src/hello app/bundle.js",
    "postjspm": "cp -R public/js/jspm_packages app && cp public/js/config.js app",
    "copy:assets": "cp -R ./public/assets app",
    "build:ui": "npm run copy:assets && npm run jspm",
    "lint:server": "eslint src",
    "prebuild:server": "npm run lint:server",
    "build:server": "./node_modules/.bin/babel src -d app",
    "prebuild": "npm run clean",
    "build": "npm run build:server && npm run build:ui",
    "pretest": "npm run build",
    "test": "echo success",
    "start": "node app/server.js",
    "restart:server": "npm run build:server && npm run start",
    "precomplete": "npm run build",
    "complete": "npm run start",
    "watch:ui": "watch 'npm run build:ui && echo === watching ==='  ./public/assets ./public/js/src --wait=1",
    "env": "env"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "lokijs": "^1.3.19",
    "method-override": "^2.3.6",
    "promise": "^7.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^2.13.1",
    "eslint-config-google": "^0.6.0",
    "jspm": "^0.16.39",
    "watch": "^0.19.1"
  },
  "jspm": {
    "directories": {
      "baseURL": "public/js"
    },
    "dependencies": {
      "immstruct": "npm:immstruct@^2.0.0",
      "omniscient": "npm:omniscient@^4.1.1",
      "react": "npm:react@^15.2.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
